<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        let numero = 0;
        
        alert("Gerador de tabuada");
        numero = prompt("Que número você gostaria de saber a tabuada(1 a 10)?");

        let resultado = "";

        for(let i = 1; i <= 10; i++) {
            resultado += (`${numero} x ${i} = ${numero * i}\n`);
        }

        alert(resultado);
    </script>
</body>
</html>
